{
    "name" : "ict/secrets",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "6.*",
        "symfony/dotenv": "6.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.*",
        "symfony/runtime": "6.*",
        "symfony/yaml": "6.*"
    },
    "authors": [
        {
            "name": "Nacho Colomina",
            "email": "ignacio.colomina@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ict\\Secrets\\": ""
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        }
    }
}
